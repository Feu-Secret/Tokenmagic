{
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "semi": true,
    "printWidth": 120,
    "bracketSpacing": true,
    "overrides": [
        {
            "files": ["*.scss", "*.css"],
            "options": {
                "requirePragma": false,
                "parser": "scss"
            }
        }
    ]
}
